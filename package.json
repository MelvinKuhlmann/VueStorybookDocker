{
  "name": "Vue-Webcomponent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "webcomponent": "vue-cli-service build --target lib --name myLib src/App.vue",
    "storybook": "start-storybook --ci -p 9001",
    "build-storybook": "build-storybook -c .storybook -o .storybookbuild"
  },
  "dependencies": {
    "babel-preset-vue": "^2.0.2",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/vue": "^5.0.1",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
